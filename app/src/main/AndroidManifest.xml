<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.HW_reposts_and_recent">
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.HW_reposts_and_recent">
        <activity android:name="com.example.HW_reposts_and_recent.MainActivity"
            android:label="@string/auth_label">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name="com.example.HW_reposts_and_recent.RegistrationActivity"
            android:label="@string/reg_label">
        </activity>
        <activity android:name="com.example.HW_reposts_and_recent.FeedActivity"
            android:label="@string/feed_label">
        </activity>

    </application>

</manifest>